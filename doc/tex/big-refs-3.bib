@Article{babenko1993,
  author      = {A. B. Babenko},
  title       = {Collembola in polar desert landscapes of the {Devon}-island, {Canada}, nwt [{Russian}]},
  journal     = {Zoologichesky Zhurnal},
  year        = {1993},
  volume      = {72},
  number      = {8},
  pages       = {28--35},
  abstract    = {It has been demonstrated that the structure of
 Collembola populations upon the Plateau of the Devon
 Island in analogous landscapes of Palaearctic is
 very similar and there are many common features
 determining specificity of soil communities formed
 in these regions: depauperated local faunas (which
 partially consist of the same species in almost all
 Polar desert regions), absolute predominance of the
 typical arctic species belonging to only 3 families
 (Hypogastruridae, Isotomidae, and Onychiuridae),
 high densities of Collembola (much higher than in
 natural communities of the humid zone including
 tundras) etc. Nevertheless, there is similarity in
 some features of the Collembola populations from the
 Plateau and of those from the more southern Arctic
 tundra subzone. This similarity, probably, is due to
 the fact that the Plateau communities are
 extrazonal. Some of these features can be nothing
 but regional peculiarities of the Canadian High
 Arctic, but the low densities or even complete
 absence of some typical arctic forms and the lower
 relative numbers of Collembola as compared to those
 of some groups of Acari are believed to be explained
