  file        = {Caterino2000ARE.pdf:home/ward/biblio/AnnRevEnt/Caterino2000ARE.pdf:PDF},
  groupsearch = {0},
  keywords    = {Hexapoda; Phylogeny; molecular phylogeny; A lire},
